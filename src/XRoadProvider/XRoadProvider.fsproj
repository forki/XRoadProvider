<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>XRoadProvider</AssemblyName>
    <RootNamespace>XRoad</RootNamespace>
    <TargetFramework>net461</TargetFramework>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <NoWarn>$(NoWarn)</NoWarn>
    <DefineConstants>$(DefineConstants);PRINT_IL</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(OS)' == 'Unix' ">
    <FrameworkPathOverride>/usr/lib/mono/4.6.1-api</FrameworkPathOverride>
  </PropertyGroup>
  <PropertyGroup>
    <Authors>Janno Põldma</Authors>
    <Copyright>Copyright © 2017</Copyright>
    <Description>Type providers for generating types and service interfaces for XRoad producers.</Description>
    <PackageIconUrl>https://raw.githubusercontent.com/janno-p/XRoadProvider/master/docs/files/img/logo.png</PackageIconUrl>
    <PackageLicenseUrl>http://github.com/janno-p/XRoadProvider/blob/master/LICENSE.txt</PackageLicenseUrl>
    <PackageProjectUrl>http://github.com/janno-p/XRoadProvider</PackageProjectUrl>
    <PackageTags>F#;fsharp;x-road;xroad;typeproviders;x-tee;xtee</PackageTags>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Common.Logging" Version="3.4.1" />
    <PackageReference Include="FSharp.Core" Version="4.2.3" />
    <PackageReference Include="NodaTime" Version="2.2.3" />
    <PackageReference Include="Optional" Version="3.2.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\paket-files\fsprojects\FSharp.TypeProviders.SDK\src\ProvidedTypes.fs">
      <Paket>True</Paket>
      <Link>paket-files/ProvidedTypes.fs</Link>
    </Compile>
    <Compile Include="AssemblyInfo.fs" />
    <Compile Include="AssemblyInfo.Build.fs" />
    <Compile Include="Attributes.fs" />
    <Compile Include="Commons.fs" />
    <Compile Include="EmitterDsl.fs" />
    <Compile Include="Emitter.fs" />
    <Compile Include="Serialization.fs" />
    <Compile Include="ServiceDescription.fs" />
    <Compile Include="TypeSchema.fs" />
    <Compile Include="CodeDom.fs" />
    <Compile Include="TypeBuilderContext.fs" />
    <Compile Include="CodeDomGenerator.fs" />
    <Compile Include="ProducerDefinition.fs" />
    <Compile Include="SecurityServer.fs" />
    <Compile Include="XRoadProvidedTypes.fsi" />
    <Compile Include="XRoadProvidedTypes.fs" />
  </ItemGroup>
</Project>